  }
  if(exists("df39") == TRUE){
    dfrows <- rbind(dfrows,df39)
  }
  if(exists("df40") == TRUE){
    dfrows <- rbind(dfrows,df40)
  }
  
  legend(x="bottomleft", legend=c(time1, time2, time3),fil=c("#1600f7","#e30091","#d90000"), title="Early Migration Events")
  
  outrows <- nrow(outrows)
  totalrows <- nrow(dfrows)
  estgf <- (outrows/totalrows)*100
  print("The estimated percent gene flow for this dataset is:")
  print(estgf)
  print("%")
  print("Code written by Skylar Gay, University of Georgia, Genetics Deptartment")
}


starttime <- Sys.time()
outlyr(df,dfcoor,"January","February","March")
