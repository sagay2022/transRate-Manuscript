dfasia <- rbind(df9,df21,df24,df27,df29)
dfamericas <- rbind(df1,df2,df3,df5,df7,df8,df12,df15,df18,df22,df30)
dfoceania <- (df11)
dfeurope <- rbind(df4,df6,df10,df13,df14,df16,df17,df19,df20,df23,df25,df26,df28,df31,df32,df33,df34,df35)
df <- rbind(dfasia,dfamericas,dfoceania,dfeurope)
outlyr <- function(df,dfcoor,time1,time2,time3){
  library(geosphere)
  library(maps)
  par(mar=c(0,0,0,0))
  maps::map('world',
            col="#b3b3b3", fill=TRUE, bg="#F1F1F0", lwd=0.05,
            mar=rep(0,4),border=0, ylim=c(-80,80) 
  )
  df1 <- subset(df,clade == "1")
  if (is.na(df1$longitude[1]) == FALSE){
    dfcoor1 <- subset(dfcoor,clade == "1")
    dfout1 <- subset(df1,longitude != dfcoor1$longitude[1])
    dfoutplot <- dfout1[!duplicated(dfout1$country),]
    points(x=dfcoor1$longitude, y=dfcoor1$latitude, col=c("black"), cex=(.7), pch=16)
    points(x=dfoutplot$longitude, y=dfoutplot$latitude, col=c("black"), cex=(.2), pch=16)
    dfclong <- as.data.frame(dfcoor1$longitude)
    dfclat <- as.data.frame(dfcoor1$latitude)
    dfclong <- as.numeric(dfclong)
    dfclat <- as.numeric(dfclat)
    dflong <- as.data.frame(dfoutplot$longitude)
    dflat <- as.data.frame(dfoutplot$latitude)
    dftime <- as.data.frame(dfoutplot$timepoint)
    
    if (is.na(dflong[1,]) == FALSE){
      dftimep <- as.numeric(dftime[1,])
      color <- if(dftimep == 1){
        "#1600f7"
      } else if(dftimep == 2){
        "#e30091"
      } else {
        "#d90000"
      }
      dflong1 <- as.numeric(dflong[1,])
      dflat1 <- as.numeric(dflat[1,])
      inter <- gcIntermediate(c(dfclong,dfclat), c(dflong1,dflat1),n=500, addStartEnd=T, breakAtDateLine=F)
      inter <- data.frame(inter)
      lines(subset(inter,lon>=0),col=c(color), lwd=1.9)
      lines(subset(inter,lon<0),col=c(color), lwd=1.9) 
    }
    if (is.na(dflong[2,]) == FALSE){
      dftimep <- as.numeric(dftime[2,])
      color <- if(dftimep == 1){
        "#1600f7"
      } else if(dftimep == 2){
        "#e30091"
      } else {
        "#d90000"
      }
      dflong1 <- as.numeric(dflong[2,])
      dflat1 <- as.numeric(dflat[2,])
      inter <- gcIntermediate(c(dfclong,dfclat), c(dflong1,dflat1),n=500, addStartEnd=T, breakAtDateLine=F)
      inter <- data.frame(inter)
      lines(subset(inter,lon>=0),col=c(color), lwd=1.9)
      lines(subset(inter,lon<0),col=c(color), lwd=1.9) 
    }
    if (is.na(dflong[3,]) == FALSE){
      dftimep <- as.numeric(dftime[3,])
      color <- if(dftimep == 1){
        "#1600f7"
      } else if(dftimep == 2){
        "#e30091"
      } else {
        "#d90000"
      }
      dflong1 <- as.numeric(dflong[3,])
      dflat1 <- as.numeric(dflat[3,])
      inter <- gcIntermediate(c(dfclong,dfclat), c(dflong1,dflat1),n=500, addStartEnd=T, breakAtDateLine=F)
      inter <- data.frame(inter)
      lines(subset(inter,lon>=0),col=c(color), lwd=1.9)
      lines(subset(inter,lon<0),col=c(color), lwd=1.9) 
    }
    if (is.na(dflong[4,]) == FALSE){
      dftimep <- as.numeric(dftime[4,])
      color <- if(dftimep == 1){
        "#1600f7"
      } else if(dftimep == 2){
        "#e30091"
      } else {
        "#d90000"
      }
      dflong1 <- as.numeric(dflong[4,])
      dflat1 <- as.numeric(dflat[4,])
