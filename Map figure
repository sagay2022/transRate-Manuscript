timechar1 <- "2020.01"
timechar2 <- "2020.02"
timechar3 <- "2020.03"
dftime1sub <- df[grep(timechar1, df$Name), ]
dftime1sub$timepoint <- 1
dftime2sub <- df[grep(timechar2, df$Name), ]
dftime2sub$timepoint <- 2
dftime3sub <- df[grep(timechar3, df$Name), ]
dftime3sub$timepoint <- 3
df <- rbind(dftime1sub,dftime2sub,dftime3sub)

