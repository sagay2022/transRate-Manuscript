  df15 <- subset(df,clade == "15")
  if (is.na(df15$longitude[1]) == FALSE){
    dfcoor1 <- subset(dfcoor,clade == "15")
    dfout15 <- subset(df15,longitude != dfcoor1$longitude[1])
    dfoutplot <-dfout15[!duplicated(dfout15$country),]
    points(x=dfcoor1$longitude, y=dfcoor1$latitude, col=c("black"), cex=(.7), pch=16)
    points(x=dfoutplot$longitude, y=dfoutplot$latitude, col=c("black"), cex=(.2), pch=16)
    dfclong <- as.data.frame(dfcoor1$longitude)
    dfclat <- as.data.frame(dfcoor1$latitude)
    dfclong <- as.numeric(dfclong)
    dfclat <- as.numeric(dfclat)
    dflong <- as.data.frame(dfoutplot$longitude)
    dflat <- as.data.frame(dfoutplot$latitude)
    dftime <- as.data.frame(dfoutplot$timepoint)
    
    if (is.na(dflong[1,]) == FALSE){
      dftimep <- as.numeric(dftime[1,])
      color <- if(dftimep == 1){
        "#1600f7"
      } else if(dftimep == 2){
        "#e30091"
      } else {
        "#d90000"
      }
      dflong1 <- as.numeric(dflong[1,])
      dflat1 <- as.numeric(dflat[1,])
      inter <- gcIntermediate(c(dfclong,dfclat), c(dflong1,dflat1),n=500, addStartEnd=T, breakAtDateLine=F)
      inter <- data.frame(inter)
      lines(subset(inter,lon>=0),col=c(color), lwd=1.9)
      lines(subset(inter,lon<0),col=c(color), lwd=1.9) 
    }
    if (is.na(dflong[2,]) == FALSE){
      dftimep <- as.numeric(dftime[2,])
      color <- if(dftimep == 1){
        "#1600f7"
      } else if(dftimep == 2){
        "#e30091"
      } else {
        "#d90000"
      }
