      inter <- data.frame(inter)
      lines(subset(inter,lon>=0),col=c(color), lwd=1.9)
      lines(subset(inter,lon<0),col=c(color), lwd=1.9) 
    }
    dfout35 <-  subset(dfout35, dfout35$continent != dfcoor1$continent[1])
  }
