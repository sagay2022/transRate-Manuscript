)

#TABLE 3
plot(tab3$input,tab3$est,,col="white",lwd=0,xlab="Migration Rate Input as %", ylab="Transmission Rate Estimation as %")
lines(tab3$input,tab3$lower,col="#1600f7",lwd=2)
